{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "lib": [
      "dom",
      "es2015",
      "es5",
      "es6"
    ]
  },
  "exclude": [
    "typings",
    "../typings"
  ],
  "types": [
    "core-js"
  ]
}